{
  "name": "discordbot_v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "wrangler dev",
    "test": "echo 'running tests 🧪' && jest --coverage && echo '✅ All tests passed'",
    "test-watch": "jest . --watch",
    "login": "wrangler login",
    "check": "wrangler whoami",
    "deploy": "wrangler deploy",
    "lint-check": "echo 'checking your code for linting issues 🤔' && eslint . && echo '✅ Your code passes lint check'",
    "lint-fix": "eslint --fix .",
    "format-check": "echo 'Checking the formatting of your code 👨‍💻' && prettier --check . && echo '✅ code matches prettier formatting'",
    "format-fix": "prettier --write .",
    "fix": "npm run lint-fix && npm run format-fix",
    "ngrok": "ngrok http 8787",
    "register": "ts-node-esm src/register.ts",
    "env-var-check": "ts-node-esm config/envVarCheck.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "ngrok": "^5.0.0-beta.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "wrangler": "^3.1.1"
  },
  "dependencies": {
    "@tsndr/cloudflare-worker-jwt": "^2.2.1",
    "discord-interactions": "^3.2.0",
    "dotenv": "^16.0.3",
    "itty-router": "^3.0.11",
    "node-fetch": "^3.3.0"
  },
  "pre-commit": [
    "lint-check",
    "format-check",
    "test"
  ],
  "volta": {
    "node": "18.12.1"
  }
}
